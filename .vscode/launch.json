{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/app.app",
            "console": "integratedTerminal",
            "args": [
                "-m","app.app"
            ]
        }
    ]
}